@model RegisterModel

<div class="form-group mb-3">
    <label asp-for="UserType" class="form-text mb-3">User Type</label>
    <select id="userType" asp-for="UserType" class="form-control" >
        <option>--Select option--</option>
        <option value="Student">Student</option>
        <option value="Staff">Staff</option>
        <option value="Visitor">Visitor</option>
    </select>
    <span asp-validation-for="UserType" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Firstname" class="form-text mb-3">First name</label>
    <input asp-for="Firstname" class="form-control" />
    <span asp-validation-for="Firstname" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Lastname" class="form-text mb-3">Last name</label>
    <input asp-for="Lastname" class="form-control" />
    <span asp-validation-for="Lastname" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="Username" class="form-text mb-3">User name</label>
    <input asp-for="Username" class="form-control"/>
    <span asp-validation-for="Username" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="Password" class="form-text mb-3">Password</label>
    <input asp-for="Password" class="form-control" />
</div>
<div class="mb-3">
    <label asp-for="ConfirmPassword" class="form-text mb-3">Confirm Password</label>
    <input asp-for="ConfirmPassword" class="form-control" />
    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="ContactNumber" class="form-text mb-3">Contact number</label>
    <input asp-for="ContactNumber" class="form-control" />
    <span asp-validation-for="ContactNumber" class="text-danger"></span>
</div>
<div class="mb-3">
    <label asp-for="DOB" class="form-text mb-3">Date of Birth</label>
    <input asp-for="DOB" class="form-control" type="date" />
    <span asp-validation-for="DOB" class="text-danger"></span>
</div>

<div class="form-group mb-3">
    <label asp-for="Identifier" class="form-text mb-3">Identifier</label>
    <input asp-for="Identifier" class="form-control" />
    <span asp-validation-for="Identifier" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="Email" class="form-text mb-3">Email</label>
    <input asp-for="Email" class="form-control" />
    <span asp-validation-for="Email" class="text-danger"></span>
</div>

